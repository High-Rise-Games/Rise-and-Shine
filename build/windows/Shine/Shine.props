<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <CUGLDir>$(SolutionDir)..\..\cugl\</CUGLDir>
    <GameDir>$(SolutionDir)..\..\</GameDir>
    <AssetDir>$(SolutionDir)..\..\assets\</AssetDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="CUGLDir">
      <Value>$(CUGLDir)</Value>
    </BuildMacro>
    <BuildMacro Include="GameDir">
      <Value>$(GameDir)</Value>
    </BuildMacro>
    <BuildMacro Include="AssetDir">
      <Value>$(AssetDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
